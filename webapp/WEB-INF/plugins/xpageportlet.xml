<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

   <!-- Plugin Informations -->
   <name>xpageportlet</name>
   <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
   <version>1.0.1</version>
   <description>xpageportlet.plugin.description</description>
   <documentation/>
   <installation/>
   <changes/>
   <user-guide/>
   <provider>xpageportlet.plugin.provider</provider>
   <provider-url>http://lutece.paris.fr</provider-url>
   <icon-url>images/admin/skin/plugins/xpageportlet/xpageportlet.png</icon-url>
   <copyright>Copyright 2001-2012 Mairie de Paris</copyright>
   <core-version-dependency>
		<min-core-version>3.1.3</min-core-version>
		<max-core-version/>	
   </core-version-dependency>
   <db-pool-required>0</db-pool-required>

   <!-- Portlet parameters -->
   <portlets>
     <portlet>
       <portlet-class>fr.paris.lutece.plugins.xpageportlet.business.portlet.XPagePortletHome</portlet-class>
	   <portlet-type-id>XPAGE_PORTLET</portlet-type-id>
	   <portlet-type-name>xpageportlet.portlet.name</portlet-type-name>
           <portlet-creation-url>plugins/xpageportlet/CreatePortletXPage.jsp</portlet-creation-url>
           <portlet-update-url>plugins/xpageportlet/ModifyPortletXPage.jsp</portlet-update-url>
           <portlet-create-action-url>plugins/xpageportlet/DoCreatePortletXPage.jsp</portlet-create-action-url>
           <portlet-modify-action-url>plugins/xpageportlet/DoModifyPortletXPage.jsp</portlet-modify-action-url>
           <portlet-create-script-template>/admin/portlet/script_create_portlet.html</portlet-create-script-template>
           <portlet-modify-script-template>/admin/portlet/script_modify_portlet.html</portlet-modify-script-template>
           <portlet-create-specific-template>/admin/plugins/xpageportlet/create_portlet_xpage.html</portlet-create-specific-template>
           <portlet-modify-specific-template>/admin/plugins/xpageportlet/modify_portlet_xpage.html</portlet-modify-specific-template>
           <portlet-create-specific-form-template/>
           <portlet-modify-specific-form-template/>
     </portlet>
	</portlets>
 </plug-in>